{
    "manifest_version": 2,
    "name": "Nine",
    "description": "Nine keys chording keypad for one handed typing",
    "version": "0.0.1",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./keypress-2.1.4.min.js", "./content.js"]
        }
    ]
}
